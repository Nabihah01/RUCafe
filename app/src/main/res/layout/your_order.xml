<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/viewYourOrder"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:backgroundTint="#E39696">

    <TextView
        android:id="@+id/yourOrders"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="157dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="157dp"
        android:layout_marginBottom="623dp"
        android:text="@string/yourOrders"
        android:textSize="34sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/placeOrder"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="137dp"
        android:layout_marginEnd="137dp"
        android:layout_marginBottom="12dp"
        android:backgroundTint="#E61D61"
        android:text="@string/placeOrder"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/orders_total_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/orders_total"
        tools:layout_editor_absoluteX="268dp"
        tools:layout_editor_absoluteY="145dp" />

    <TextView
        android:id="@+id/orders_total"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        tools:layout_editor_absoluteX="325dp"
        tools:layout_editor_absoluteY="145dp" />

    <TextView
        android:id="@+id/orders_subtotal_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/your_orders_subtotal_label"
        tools:layout_editor_absoluteX="245dp"
        tools:layout_editor_absoluteY="79dp" />

    <TextView
        android:id="@+id/orders_subtotal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        tools:layout_editor_absoluteX="325dp"
        tools:layout_editor_absoluteY="79dp" />

    <TextView
        android:id="@+id/orders_salestax_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/your_orders_salestax_label"
        tools:layout_editor_absoluteX="245dp"
        tools:layout_editor_absoluteY="113dp" />

    <TextView
        android:id="@+id/orders_salestax"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        tools:layout_editor_absoluteX="325dp"
        tools:layout_editor_absoluteY="113dp" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:text="@string/deleteItem"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="177dp" />

    <ListView
        android:layout_width="409dp"
        android:layout_height="473dp"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="197dp" />

</androidx.constraintlayout.widget.ConstraintLayout>